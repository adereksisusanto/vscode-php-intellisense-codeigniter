{
  "name": "server-php-intellisense-codeigniter",
  "displayName": "server-php-intellisense-codeigniter",
  "description": "Language server in node for server-php-intellisense-codeigniter",
  "author": "Ade Reksi Susanto",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "webpack-pro": "webpack --mode production",
    "webpack": "webpack --mode development",
    "installServer": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -w -p ."
  },
  "devDependencies": {
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-uri": "^3.0.2"
  }
}
